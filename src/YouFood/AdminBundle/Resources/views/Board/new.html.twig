{% extends 'YouFoodAdminBundle::layout.html.twig' %}

{% block title %}Nouvelle Table{% endblock %}

{% block content %}
<div class="page-header">
    <h1>
        Nouvelle Table
        <small>Ajouter une nouvelle table.</small>
    </h1>
</div>

<form action="{{ path('board_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p class="form-actions">
        <button class="btn btn-large btn-success" type="submit">Créer</button>
        <a class="btn btn-large" href="{{ path('board') }}">Retour à la liste des tables</a>
    </p>
</form>

{% endblock %}
